    <string name="tutorial_app_name">About trusted face</string>
    <string name="tutorial_header">Look at your phone to unlock it.</string>
    <string name="tutorial_info_text">"Keep these things in mind:

 "<li><ul>"Face matching is less secure than a pattern, PIN or password.

 "<li>Someone who looks like you could unlock your phone.</li>"

 "<li>The data used to recognise your face is kept private on the phone.</li>"

 "</ul></li></string>
    <string name="tutorial_continue_label">Setup</string>
    <string name="intro_header">For best results:</string>
    <string name="intro_info_text">" "<li><ul>"Find an indoor spot that is not too bright or too dim.

 "<li>Hold the phone at eye level.</li>"

 "</ul></li></string>
    <string name="intro_continue_label">Next</string>
    <string name="endscreen_header">Ready to go!</string>
    <string name="endscreen_info_text">When you want to unlock, just turn on your screen and show your face to the camera.</string>
    <string name="endscreen_bullet_one_text">Your phone unlocks as soon as it recognises you.</string>
    <string name="endscreen_bullet_two_text">"If your phone doesn't recognise your face, you can unlock with your pattern, PIN or password."</string>
    <string name="endscreen_tip_text">Tip: Improve face matching by showing what you look like in different conditions.</string>
    <string name="endscreen_continue_label">OK</string>
