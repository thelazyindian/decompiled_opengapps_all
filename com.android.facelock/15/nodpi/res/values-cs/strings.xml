    <string name="endscreen_bullet_two_text">Pokud telefon váš obličej nerozezná, můžete jej odemknout pomocí gesta, kódu PIN nebo hesla.</string>
    <string name="endscreen_tip_text">Tip: Zlepšete rozpoznávání obličeje tím, že ukážete, jak vypadáte za různých podmínek.</string>
