    <string name="tutorial_header">Lihat ponsel Anda untuk membuka gemboknya.</string>
    <string name="tutorial_info_text">"Ingatlah hal-hal berikut:

 "<li><ul>"Pencocokan wajah kurang aman dibandingkan pola, PIN, atau sandi.

 "<li>Seseorang yang mirip dengan Anda dapat membuka kunci ponsel.</li>"

 "<li>Data yang digunakan untuk mengenali wajah Anda dijaga agar tetap bersifat pribadi di ponsel.</li>"

 "</ul></li></string>
    <string name="tutorial_continue_label">Siapkan</string>
    <string name="intro_header">Untuk hasil terbaik:</string>
    <string name="intro_info_text">" "<li><ul>"Cari lokasi dalam ruangan yang tidak terlalu terang atau terlalu redup.

"<li>Pegang ponsel sejajar dengan mata.</li>"

"</ul></li></string>
    <string name="intro_continue_label">Berikutnya</string>
    <string name="endscreen_header">Semua siap!</string>
    <string name="endscreen_info_text">Jika ingin membuka kunci, cukup aktifkan layar dan perlihatkan wajah Anda ke kamera.</string>
    <string name="endscreen_bullet_one_text">Ponsel akan segera terbuka setelah mengenali wajah Anda.</string>
    <string name="endscreen_bullet_two_text">Jika ponsel tidak mengenali wajah Anda, buka dengan pola, PIN, atau sandi.</string>
    <string name="endscreen_tip_text">Kiat: Tingkatkan pencocokan wajah dengan menampilkan wajah Anda di berbagai kondisi.</string>
    <string name="endscreen_continue_label">Oke</string>
    <string name="endscreen_license_label">Lisensi</string>
