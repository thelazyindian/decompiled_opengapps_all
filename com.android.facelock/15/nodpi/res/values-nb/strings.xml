    <string name="tutorial_app_name">Om pålitelig ansikt</string>
    <string name="tutorial_header">Se på telefonen for å låse den opp.</string>
    <string name="tutorial_info_text">"Husk:

"<li><ul>"Ansiktssamsvar er mindre sikkert enn mønster, PIN-kode eller passord.

"<li>Personer som ser ut som deg, kan låse opp nettbrettet.</li>"

"<li>Dataene som brukes til å gjenkjenne ansiktet ditt, blir holdt privat på telefonen.</li>"

"</ul></li></string>
    <string name="tutorial_continue_label">Konfigurer</string>
    <string name="intro_header">Slik oppnår du best resultat:</string>
    <string name="intro_info_text">" "<li><ul>"Finn et sted innendørs som ikke har for sterk eller svak belysning.

"<li>Hold telefonen på nivå med øynene dine.</li>"

"</ul></li></string>
    <string name="intro_continue_label">Neste</string>
    <string name="endscreen_header">Alt er klart!</string>
    <string name="endscreen_info_text">Når du vil låse opp enheten, slår du bare på skjermen og viser ansiktet ditt til kameraet.</string>
    <string name="endscreen_bullet_one_text">Telefonen din låses opp når du gjenkjennes.</string>
    <string name="endscreen_bullet_two_text">Hvis telefonen ikke gjenkjenner ansiktet ditt, kan du låse den opp med mønsteret, PIN-koden eller passordet.</string>
    <string name="endscreen_tip_text">Tips: Du kan forbedre ansiktsgjenkjennelsen ved å vise hvordan du ser ut i forskjellige lysforhold.</string>
    <string name="endscreen_continue_label">OK</string>
    <string name="endscreen_license_label">Lisenser</string>
