<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="true">
        <objectAnimator android:duration="@android:integer/config_shortAnimTime" android:valueTo="@dimen/floating_action_button_pressed_elevation" android:valueType="floatType" android:propertyName="translationZ" />
    </item>
    <item android:state_focused="true" android:state_enabled="true">
        <objectAnimator android:duration="@android:integer/config_shortAnimTime" android:valueTo="@dimen/floating_action_button_pressed_elevation" android:valueType="floatType" android:propertyName="translationZ" />
    </item>
    <item>
        <objectAnimator android:duration="@android:integer/config_shortAnimTime" android:valueTo="0" android:valueType="floatType" android:propertyName="translationZ" />
    </item>
</selector>